  AddIcon /includes/icons/eslint.svg *.eslintignore *.eslintrc.js *.eslintrc.json *.eslintrc *.eslintrc.yml
  AddIcon /includes/icons/webpack.svg webpack.config.js
  AddIcon /includes/icons/rollup.svg rollup.config.js
  AddIcon /includes/icons/postcss.svg postcss.config.js *.postcssrc *.postcssrc.json *.postcssrc.js *.postcssrc.yml
  AddIcon /includes/icons/grunt.svg gruntfile.js Gruntfile.js
